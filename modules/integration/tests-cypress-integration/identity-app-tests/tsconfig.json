{
    "baseUrl": ".",
    "paths": {
        "*": ["types/*"],
        "@wso2is-testing/cypress-base/commands": ["node_modules/@wso2is-testing/cypress-base/dist/commands"],
        "@wso2is-testing/cypress-base/constants": ["node_modules/@wso2is-testing/cypress-base/dist/constants"],
        "@wso2is-testing/cypress-base/page-objects": ["node_modules/@wso2is-testing/cypress-base/dist/page-objects"],
        "@wso2is-testing/cypress-base/utils": ["node_modules/@wso2is-testing/cypress-base/dist/utils"]
    },
    "compilerOptions": {
        "target": "es5",
        "lib": [
            "es5",
            "dom"
        ],
        "types": [
            "cypress",
            "node"
        ]
    },
    "include": [
        "cypress"
    ],
    "exclude": [
        "node_modules"
    ]
}
